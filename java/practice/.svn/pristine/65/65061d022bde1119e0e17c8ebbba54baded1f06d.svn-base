package com.gsitm.practice.ex02;

import java.util.Scanner;


/** 
* @과목명					: GS ITM 인턴사원 자바교육
* @FileName				: ScannerFor_1069.java 
* @Project				: gsitm_java 
* @Date					: 2018. 3. 28. 
* @작성자					: 조현우
* @프로그램 설명			: Scanner와 for문을 이용한 '#'찍기
*/
public class ScannerFor_1069 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// 스캐너 객체를 생성하고 매개변수로 system.in을 활용하여 키보드로 정수를 받게한다.
		Scanner sc = new Scanner(System.in);
		// num이란 변수가 입력받은 숫자를 참조시킨다.
		int num = sc.nextInt();
		// 반복문을 이용하여 입력받은 숫자만큼 '#'을 출력한다.
		for(int i = 0; i < num; i++) {
			System.out.print('#');
		}
	}

}
