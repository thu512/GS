public class ScannerFor_1036 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);		//스캐너 객체 생성
		int val = sc.nextInt();								//val에 입력받은 값 저장
		
		for(int i=0; i<val; i++) {							//val만큼 # 수 출력
			System.out.print('#');
		}
	}
}
