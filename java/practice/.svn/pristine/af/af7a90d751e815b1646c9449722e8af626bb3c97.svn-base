public class ScannerFor_1041 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Scanner sc = new Scanner(System.in);
		
		int i = sc.nextInt();
		
		for( int j = 1 ; j <= i ; j++) {
			System.out.print('#');
		}
		
		sc.close();
	}

