public class ScannerFor_1066 {

	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);

		int num = scanner.nextInt();
		String str = "";

		for (int i = 0; i < num; i++) {
			str += "#";
		}

		System.out.println(str);
		scanner.close();
	}

}
