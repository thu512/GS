public class ScannerFor_1052 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		StringBuilder sb = new StringBuilder();

		int num = sc.nextInt();

		for(int i=0; i<num; i++) {
			sb.append("#");
		}
		
		System.out.println(sb);
		
		sc.close();
	}

}
