public class ScannerFor_1059 {

	public static void main(String[] args) {
		// 입력받기
		Scanner sc = new Scanner(System.in);
		String inputString = sc.nextLine();
		
		// string -> int 변환
		int length = Integer.parseInt(inputString);
		
		// # 찍기
		for(int i = 0; i < length; i++) {
			System.out.print("#");
		}
	}

}
