public class ScannerFor_1043 {
	public static void main(String[] args) {
		System.out.println("숫자를 입력해주세요.");
		Scanner scanner = new Scanner(System.in);
		int input = scanner.nextInt();
		
		for(int i=0; i<input; i++) {
			
			System.out.print("*");
			
		}
	}
}
