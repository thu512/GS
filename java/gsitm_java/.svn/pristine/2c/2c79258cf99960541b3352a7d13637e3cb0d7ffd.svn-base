class Exclass_1040{
	//고양이라는 문자열을 출력하는 메소드입니다.
	/* alt + shift + j 혹은 ctrl + shift + j 활용 */
	
	public static void main(String[] args) {
		System.out.println("고양이");
		System.out.println();
		
		int i=0; // primitive 타입, 기본형
		double dd= 1.1; 
		
		Double doubl = new Double(1.1);//wrapper 클래스
		
		String var = "10";
		int var2 =Integer.parseInt(var); //wrapper 클래스
		
		//Boxing 기본타입의 값을 래핑하는 과정, 기본 값 -> 객체값
		Double dd2 = new Double(1.1);
		Double dd3 = Double.valueOf(2.2);
	
		//unBoxing 래퍼 객체에서 기본타입의 값을 얻는 과정, 객체값 -> 기본값
		double ddd = doubl.doubleValue();
		
		char a= 1;
		int b= 2;
		
	
		
		System.out.println(a+b);
	}	
}


