public class RemoteCon_1068 {

	public boolean turn(String command) {
		
		Tv_1068 tv = new Tv_1068();
		
		if(command == "on") {
			tv.setPower(true);
		}else if(command == "off"){
			tv.setPower(false);
		} else {
			System.out.println("on/off를 입력하세요.");
		}
		
		return tv.isPower();
	}

}
