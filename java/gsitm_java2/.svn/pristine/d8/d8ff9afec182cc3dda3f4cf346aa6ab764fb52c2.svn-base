public class BabyCat_1029 extends MomCat_1029 {

	//울다 함수 재정의
	@Override
	public void cry() {
		System.out.println("음메");
	}
	
	public static void main(String[] args) {
		//새끼고양이와 어미고양이 울다 함수 확인
		BabyCat_1029 babyCat = new BabyCat_1029();
		MomCat_1029 momCat = new MomCat_1029();
		
		babyCat.cry(); //음메
		momCat.cry(); //냐옹
	}
}
