public class RemoteCon_1077 {
	public boolean turn_on(){
		Tv_1077.power = true;
		System.out.println("전원이 켜졌습니다");
		return Tv_1077.power;
	}
	public boolean turn_off(){
		Tv_1077.power = false;
		System.out.println("전원이 꺼졌습니다");
		return Tv_1077.power;
	}
}
