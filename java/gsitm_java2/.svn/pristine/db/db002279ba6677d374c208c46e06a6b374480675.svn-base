public class Intern_1055 {
	
	private String name;
	private int age;
	private String apply;
	
	//생성자
	Intern_1055(){	
	}
	
	public Intern_1055(String name){
		this(name,24,"SI");
	}
	
	public Intern_1055(String name, int age){
		this(name,age,"SI");
	}
	
	public Intern_1055(String name, int age, String apply){
		this.name = name;
		this.age = age;
		this.apply = apply;
	}
	
	public static void main(String[] args) {
		Intern_1055 intern1055 = new Intern_1055("이경화",25,"SI");
	}
}

