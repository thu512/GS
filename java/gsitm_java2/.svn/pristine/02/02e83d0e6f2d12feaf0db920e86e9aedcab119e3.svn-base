public class BabyCat_1037 extends ParentCat_1037{
	
	BabyCat_1037() {
		super();
	}
	
	@Override
	public void cry() {
		System.out.println("음매");
	}
	
	public static void main(String[] args) {
		ParentCat_1037 parentCat = new ParentCat_1037();
		BabyCat_1037 babyCat = new BabyCat_1037();
		
		System.out.println("어미고양이 털색 : " + parentCat.getColor());
		// private?
		System.out.println("새끼고양이 털색? : " + babyCat.getColor());
		parentCat.cry();
		babyCat.cry();
		// parentCat.steal();
		// babyCat.steal();
	}
}
