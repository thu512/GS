public class LoginUser_1041 {
	private static  LoginUser_1041 singleton = new LoginUser_1041();
	
	private LoginUser_1041() {}
	
	private String name;
	private String id;
	private String check;
	
	static LoginUser_1041 getInstance() {
		return singleton;
		
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getCheck() {
		return check;
	}

	public void setCheck(String check) {
		this.check = check;
	}
}
