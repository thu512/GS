public class RemoteCon_1041 {
	
	Tv_1041 tv = new Tv_1041();
	
	
	boolean onOff(boolean turnon) {
		if( turnon == true) {
			tv.power = true;
		}
		if( turnon == false) {
			tv.power = false;
		}
		
		return tv.power;
	}

}
