public class Human_1037 {

	public static void main(String[] args) {
		
		Tv_1037 tv = new Tv_1037();
		RemoteCon_1037 remoteCon = new RemoteCon_1037();
		
		System.out.println(remoteCon.powerOn(tv));
		
	}
}

class Tv_1037 {
	boolean power = false;
}

class RemoteCon_1037 {
	public boolean powerOn(Tv_1037 tv) {
		tv.power = true;
		return tv.power;
	}
}