public class Intern_1057 {
	
	String name;
	int age;
	String site;
	
	public Intern_1057() {}
	

	public Intern_1057(String name) {	
		this.name = name;
	}
	
	public Intern_1057(String name,int age) {	
		this.name = name;
		this.age = age;
	}
	
	public Intern_1057(String name, int age, String site) {
		this.name = name;
		this.age = age;
		this.site = site;
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Intern_1057 intern = new Intern_1057("LEE",28,"SI");
		Intern_1057 intern2 = new Intern_1057("LEE");
		Intern_1057 intern3 = new Intern_1057("LEE",28);
		Intern_1057 intern4 = new Intern_1057("LEE",28,"SI");
	}
}
