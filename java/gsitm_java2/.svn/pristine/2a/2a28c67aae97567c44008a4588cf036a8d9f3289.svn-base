public class Lion_1036 {
	private static boolean lunchL = false;	//사자가 밥을 안먹은상태로 초기화
	
	public boolean getlunchL() {
		return this.lunchL;
	}
	
	public void setlaunchL(boolean feed) {
		lunchL = true;
	}
	
	public void isEat() {
		if(lunchL == true) {
			System.out.println("사자는 밥을 먹었습니다.");
		}else {
			System.out.println("사자는 밥을 먹지 않았습니다.");
		}
	}
	
}
