public class ParentCat_1054 {

	private String furColor;
	public String arm;
	public String leg;
	public String shoulder;
	
	/**
	 * 생성자(Constructor)
	 */
	public ParentCat_1054() {
		super();
		this.furColor = "갈색";
		this.arm = "정상";
		this.leg = "정상";
		this.shoulder = "정상";
	}

	
	/**
	 * 생성자(Constructor)
	 * @param furColor
	 * @param arm
	 */
	public ParentCat_1054(String arm) {
		super();
		this.arm = arm;
		this.leg = "정상";
		this.shoulder = "정상";
	}
	
	/**
	 * 생성자(Constructor)
	 * @param furColor
	 * @param arm
	 * @param leg
	 */
	public ParentCat_1054(String arm, String leg) {
		super();
		this.arm = arm;
		this.leg = leg;
		this.shoulder = "정상";
	}
	
	/**
	 * 생성자(Constructor)
	 * @param furColor
	 * @param arm
	 * @param leg
	 * @param shoulder
	 */
	public ParentCat_1054(String arm, String leg, String shoulder) {
		super();
		this.arm = arm;
		this.leg = leg;
		this.shoulder = shoulder;
	}

	
	//==================== FUNCTION(METHOD) ========================
	
	public void roar() {
		System.out.println(this.furColor + " 고양이가 " + "냐옹" + " 하고 웁니다");
	}
	
	private void steal() {
		System.out.println("훔치기!");
	}
	
	// =================== GETTER/SETTER ============================

	/**
	 * furColor Getter
	 * @return the furColor
	 */
	public String getFurColor() {
		return furColor;
	}


	/**
	 * furColor Setter
	 * @param furColor the furColor to set
	 */
	public void setFurColor(String furColor) {
		this.furColor = furColor;
	}


	/**
	 * arm Getter
	 * @return the arm
	 */
	public String getArm() {
		return arm;
	}


	/**
	 * arm Setter
	 * @param arm the arm to set
	 */
	public void setArm(String arm) {
		this.arm = arm;
	}


	/**
	 * leg Getter
	 * @return the leg
	 */
	public String getLeg() {
		return leg;
	}


	/**
	 * leg Setter
	 * @param leg the leg to set
	 */
	public void setLeg(String leg) {
		this.leg = leg;
	}


	/**
	 * shoulder Getter
	 * @return the shoulder
	 */
	public String getShoulder() {
		return shoulder;
	}


	/**
	 * shoulder Setter
	 * @param shoulder the shoulder to set
	 */
	public void setShoulder(String shoulder) {
		this.shoulder = shoulder;
	}

	

	
	
	
	
}
