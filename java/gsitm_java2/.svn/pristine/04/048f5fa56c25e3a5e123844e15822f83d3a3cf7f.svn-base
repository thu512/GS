public class BabyCat_1069 extends MommyCat_1069 {

	public BabyCat_1069() {
		super();
	}
	@Override
	public void howling() {
		System.out.println("음메");
	}
	public static void main(String[] args) {
		BabyCat_1069 babyCat = new BabyCat_1069();
		MommyCat_1069 mommyCat = new MommyCat_1069();
		System.out.println("애기고양이 : 털색깔(어미)" + babyCat.getBrushColor());
		babyCat.setBrushColor("blue");
		System.out.println("애기고양이 : 털색깔(자식)" + babyCat.getBrushColor());
		System.out.println("애기고양이 : 팔 " + babyCat.arms);
		System.out.println("애기고양이 : 다리 " + babyCat.leg);
		System.out.println("애기고양이 : 어깨 " + babyCat.shoulder);
		System.out.println("애기고양이 울음 ");
		babyCat.howling();
		System.out.println("어미고양이 울음 ");
		mommyCat.howling();
	}

	
	
	

}
