public class Intern_1040 {

	String name;
	int age;
	String site;
	
	
	Intern_1040(){		 			//디폴트 생성자
	this("노종범", 30, "GS-Retail");	
	}
	
	Intern_1040(String name){	
	this(name, 30, "GS-Retail");
	}
	
	Intern_1040(String name, int age){	
	this(name, age, "GS-Retail");
	}
	
	Intern_1040(String name, int age, String site){	
	this.name = name;
	this.age = age;
	this.site = site;
	}
	
	public static void main(String[] args) {
		
		Intern_1040 i1 = new Intern_1040();
		
		System.out.println(i1.name);
		System.out.println(i1.age);
		System.out.println(i1.site);
		

		
	}
	
}